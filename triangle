#include <iostream>
 
using namespace std;
 
int main()
{
  int n = 3;
  int a[n];
  for( int i=0 ; i<n ; i++ )
  {
      cin >> a[i];
  }
  if( a[0] == a[1] && a[1] == a[2] )
  {
      cout << "Равносторонний" << endl;
  }
  else if( a[0] == a[1] || a[1]==a[2] || a[0]==a[2])
  {
      cout << "Равнобедренный" << endl;
  }
  else
  {
      cout << "Разносторонний" << endl;
  }
  return 0;
}
